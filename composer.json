{
    "name": "sellerworks/amazon-mws",
    "description": "A PHP client for the Amazon Marketplace Web Services (MWS) API",
    "keywords": ["Amazon", "MWS", "SellerCentral"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Steve Nebes",
            "email": "steve@gmail.com",
            "homepage": "https://github.com/sellerworks"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "ext-curl": "*",
        "ext-soap": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^6.0",
        "psr/log": "1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "phpstan/phpstan": "^0.11",
        "symfony/var-dumper": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "SellerWorks\\Amazon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SellerWorks\\Amazon\\Tests\\": "tests/"
        }
    }
}
